{
    "name": "haemanthus/codeigniter3-ide-helper",
    "description": "Dynamically add type hints to your CodeIgniter 3 controllers and models",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "nikic/php-parser": "^3.1",
        "mnapoli/silly": "^1.5",
        "php-di/php-di": "^5.4",
        "league/pipeline": "^0.3.0",
        "symfony/finder": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^1.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Haemanthus\\CodeIgniter3IdeHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yusuf T.",
            "email": "dev.yusuftaufiq@gmail.com"
        }
    ],
    "bin": [
        "bin/ide-helper"
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/yusuftaufiq/codeigniter3-ide-helper"
}
